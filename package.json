{
  "name": "open_stream",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001 --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:dev": "npx dotenv -e .env.dev -- npx prisma migrate dev",
    "migrate:reset": "npx dotenv -e .env.dev -- npx prisma migrate reset",
    "migrate:all": "npx dotenv -e .env.dev -- npx prisma migrate reset && npx dotenv -e .env.dev -- npx prisma migrate dev"
  },
"dependencies": {
    "dotenv": "^16.4.7",
    "next": "^15.2.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
},
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20.17.24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "next": "^15.2.2",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
